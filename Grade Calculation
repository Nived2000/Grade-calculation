#include <stdio.h>

int main()
{
    int a;
    printf("Enter your marks : ");
    scanf("%d",&a);
    if(a>100)
    {
        printf("Invalid Entry");
    }
    else if(a>85)
    {
        printf("Your grade is A");
    }
    else if(a>70)
    {
        printf("Your grade is B");
    }
    else if(a>55)
    {
        printf("Your grade is C");
    }
    else if(a>40)
    {
        printf("Your grade is D");
    }
    else
    {
        printf("You have failed");
    }
    return 0;
}
